# Application settings
ENVIRONMENT=development
PYTHONPATH=.

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json  # or text

# Redis settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# RabbitMQ settings
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VIRTUAL_HOST=/

# DMarket settings
DMARKET_API_URL=https://api.dmarket.com
DMARKET_COMMISSION_PERCENT=7.0
PROFIT_THRESHOLD_USD=-30

# Telegram Bot settings
TELEGRAM_API_TOKEN=your_bot_token_from_botfather
TELEGRAM_WHITELIST=123456789,987654321

# Proxy settings
USE_PROXY=false
PROXY_FILE_PATH=utils_mount/dmarket_proxies.txt

# Tracing settings (optional)
ENABLE_TRACING=false
ZIPKIN_ADDRESS=http://localhost:9411/api/v2/spans

# Worker settings
WORKER_CONCURRENCY=10
WORKER_PREFETCH_COUNT=100 